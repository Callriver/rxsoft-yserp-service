<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rxsoft.dao.AttributeClassifyMapper">


    <update id="update">
        UPDATE attribute_classify set attribute_name=#{attribute_name},classify_id=#{classify_id} WHERE attribute_id=#{attribute_id}
    </update>
    <select id="list" resultType="AttributeClassify">
    	select * from attribute_classify
    </select>
    <insert id="add">
    	insert into attribute_classify values(#{classify_id},#{classify_name})
    </insert>
    <delete id="delete">
    	delete from attribute where classify_id=#{classify_id}
    </delete>
</mapper>
